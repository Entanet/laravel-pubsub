{
  "name": "leroy-merlin-br/laravel-pubsub",
  "description": "A Pub-Sub abstraction for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Superbalist.com a division of Takealot Online (Pty) Ltd",
      "email": "info@superbalist.com"
    }
  ],
  "conflict": {
    "superbalist/laravel-pubsub": "*"
  },
  "require": {
    "php": ">=7.4.0",
    "google/cloud": "^0.149.0",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/console": "^8.0",
    "illuminate/support": "^8.0",
    "superbalist/php-pubsub": "^1.0|^2.0",
    "superbalist/php-pubsub-redis": "^1.0|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "LeroyMerlin\\LaravelPubSub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
